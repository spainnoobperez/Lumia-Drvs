[Version]
Signature   = "$WINDOWS NT$"
Class       = System
ClassGuid   = {4d36e97d-e325-11ce-bfc1-08002be10318}
Provider    = %COMP%
DriverVer   = 09/26/2013,1.0.3051.0000
CatalogFile = hwnled.cat

[SourceDisksNames]
3426 = windows cd

[SourceDisksFiles]
hwnled.sys = 3426

;12 == Windows\System32\Drivers
[DestinationDirs]
DefaultDestDir = 12

[ControlFlags]
ExcludeFromSelect = *

;*****************************************
; HWNLED driver Install Section
;*****************************************

[Manufacturer]
%COMP% = %COMP%,NTARM

[Qualcomm.NTARM]
%hwnled.DeviceDesc% = hwnled_Device,ACPI\QCOM0D50

[hwnled_Device.NT]
Copyfiles = hwnledCopyFiles

[hwnledCopyFiles]
hwnled.sys, , , 0x100,

[hwnled_Device.NT.Services]
AddService = hwnled,%SPSVCINST_ASSOCSERVICE%,hwnled_Service_Inst

[hwnled_Service_Inst]
DisplayName    = %hwnled.SVCDESC%
ServiceType    = 1               ; SERVICE_KERNEL_DRIVER
StartType      = 3               ; SERVICE_DEMAND_START
ErrorControl   = 1               ; SERVICE_ERROR_NORMAL
ServiceBinary  = %12%\hwnled.sys
LoadOrderGroup = Extended Base

[Strings]
COMP                   = "Qualcomm"
SPSVCINST_ASSOCSERVICE = 0x00000002
hwnled.DeviceDesc      = "hwnled Driver"
hwnled.SVCDESC         = "hwnled Service"



